<table>
<% @tes.each_with_index do |player, index| %>
  <tr>
     <td><%= "#{index + 1}. " %></td>
     <td><%= player.name %></td>
     <td><%= player.position %></td>
     <td><%= player.projection %></td>
     <td><%= player.dropoff %></td>
  </tr>
<% end %>
</table>

